rch Products</li>\n            <Button type=\"primary\" shape=\"round\" href=\"/dashboard\">Search</Button>\n            <li></li>\n            \n            <li><div className=\"divider\"></div></li>\n\n            <li className=\"subheader\">Upload Product</li>\n            <Button type=\"primary\" shape=\"round\" icon={<UploadOutlined />} href=\"/product/upload\"></Button>\n\n            <li><div className=\"divider\"></div></li>\n\n            <li className=\"subheader\">Go to Cart</li>\n            <Badge count={0} >\n                <Button type=\"primary\" shape=\"round\" icon={<ShoppingCartOutlined />} href=\"/user/cart\"></Button>\n            </Badge>\n            <li><div className=\"divider\"></div></li>\n\n        </ul>\n        \n\n\n\n    \n    </div>\n\n\n    )\n\n \n}\n\nexport default Header; \n","/Users/josh/Documents/UTCode/Project3/MainHustle/client/src/components/Dashboard.js",["75"],"/Users/josh/Documents/UTCode/Project3/MainHustle/client/src/components/FrontPage.js",["76","77","78"],"/Users/josh/Documents/UTCode/Project3/MainHustle/client/src/components/ProductPage.js",["79"],"import React, {useState, useEffect, useContext} from 'react'\nimport Axios from 'axios'\nimport {Row, Col, Descriptions, Button } from 'antd'\n// import ImageGallery from 'react-image-gallery';\nimport  {CartContext} from '../contexts/CartContext';\n// import ProdPageImage from './ProdPageImage'\n\n\nfunction ProductPage(props) {\n\n \n    const productId = props.match.params.productId\n    const [product, setProduct] = useState([])\n    const { addProduct, cartItems, increase } = useContext(CartContext);\n\n    console.log(product.image)\n    useEffect(() => {\n\n        Axios.get(`/api/product/products_by_id?id=${productId}&type=single`)\n            .then(response => {\n                setProduct(response.data[0])\n                console.log(response.data[0])\n            })\n    },[])\n\n   \n\n\n\n\n    const isInCart = product => {\n        return !!cartItems.find(item => item.id === product.id);\n        }\n\n        \n    return (\n        <div style={{width: '100%', padding: '3rem 4rem'}}>\n            <div style={{textAlign: 'center', marginBottom: '3rem'}}>\n                <h4>{product.title}</h4>\n            </div>\n        <br />\n\n    <Row gutter={[16, 16]}>\n            {/* <Col lg={12} xs={24}> */}\n                    {/* Product Image */}\n               {/* <ImageGallery  items={image}/> */}\n                {/* <img style={{width: '100%'}} alt=\"ProductImg\" src={`${product.image}`} />  */}\n                {/* <ProdPageImage product={product} />\n                 */}\n            {/* </Col> */}\n\n\n            <Col lg={24} xs={24}>\n\n\n\n                <Descriptions\n                    title=\"Product Info\"\n                    bordered\n                    layout=\"vertical\"\n                    >\n                    <Descriptions.Item label=\"Category\">{product.category}</Descriptions.Item>\n                    <Descriptions.Item label=\"Product\">{product.title}</Descriptions.Item>\n                    <Descriptions.Item label=\"Amount\">${product.price}</Descriptions.Item>\n                    <Descriptions.Item label=\"Description\">{product.description}</Descriptions.Item>\n                </Descriptions>\n                    <br />\n\n               \n                \n                {\n                    isInCart(product) && \n                    <Button \n                    onClick={() => increase(product)}\n                    type=\"primary\"\n                    shape=\"round\">Add on to Cart</Button>\n                }\n\n\n                {\n                    !isInCart(product) && \n                    <Button \n                    onClick={() => addProduct(product)}\n                    type=\"primary\"\n                    shape=\"round\">Add to Cart</Button>\n                }\n\n            </Col>\n\n           \n\n    </Row>\n               \n        </div>\n    )\n}\n\n\n\n